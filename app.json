{
  "expo": {
    "name": "eatsooon",
    "displayName": "Eatsooon",
    "slug": "eatsooon",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "eatsooon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.miqui.eatsooon",
      "appleTeamId": "88NCV6QJ5S",
      "buildNumber": "6",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "processing"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA"],
      "package": "com.miqui.eatsooon",
      "versionCode": 6,
      "privacyPolicy": "https://miqui.com/privacy-policy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization",
      "expo-notifications",
      "expo-background-task"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "694a6a8d-9603-4417-a237-d4ab1400986a"
      }
    }
  }
}
